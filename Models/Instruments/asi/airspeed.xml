<?xml version="1.0"?>

<PropertyList>

<path>stby-asi.ac</path>

  <animation>
  <type>scale</type>
  <x-offset>0.90</x-offset>
  <y-offset>0.90</y-offset>
  <z-offset>0.90</z-offset>
 </animation>
<animation>
 <type>material</type>
 <object-name>Face</object-name>
 <object-name>Needle1</object-name>
 <emission>
  <red>0.5</red>
  <green>0.5</green>
  <blue>0.5</blue>
  <factor-prop>controls/lighting/instruments-norm</factor-prop>
 </emission>
</animation>

 <animation>
  <type>material</type>
  <object-name>bezel</object-name>
  <object-name>cylinder</object-name>
  <emission>
   <red>0.5</red>
   <green>0.5</green>
   <blue>0.5</blue>
     </emission>
 </animation>
 
 
<animation>
 <type>rotate</type>
 <object-name>Needle1</object-name>
 <property>/instrumentation/airspeed-indicator/indicated-speed-kt</property>
 <interpolation>
  <entry>
   <ind>0</ind>
   <dep>-9.5</dep>
  </entry>
  <entry>
   <ind>60</ind>
   <dep>0</dep>
  </entry>
  <entry>
   <ind>250</ind>
   <dep>244</dep>
  </entry>
  <entry>
   <ind>300</ind>
   <dep>265.5</dep>
  </entry>
  <entry>
   <ind>450</ind>
   <dep>331</dep>
  </entry>
 </interpolation>
 <axis>
  <x>-1</x>
  <y>0</y>
  <z>0</z>
 </axis>
</animation>

</PropertyList>


