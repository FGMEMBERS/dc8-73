        <position>
            <x>0</x>
            <y>0</y>
            <z>-2</z>
        </position>
        <reference-dist>5.0</reference-dist>
        <max-dist>10.0</max-dist>
    </gear1>

    <gear1>
        <name>gear-lock</name>
        <path>Sounds/gear-lck.wav</path>
        <condition>
            <equals>
                <property>/gear/gear[0]/position-norm</property>
                <value>0</value>
            </equals>
        </condition>
        <volume>
            <property>sim/sound/volume</property>
        </volume>
<position>
            <x>0</x>
            <y>0</y>
            <z>-2</z>
        </position>
